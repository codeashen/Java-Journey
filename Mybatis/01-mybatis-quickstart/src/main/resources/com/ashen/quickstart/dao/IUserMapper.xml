<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 指定namespace，说明关联的是哪个dao -->
<mapper namespace="com.ashen.quickstart.dao.IUserDao">

    <!-- 查询所有 -->
    <select id="findAll" resultType="com.ashen.quickstart.domain.User">
        select * from user
    </select>
</mapper>